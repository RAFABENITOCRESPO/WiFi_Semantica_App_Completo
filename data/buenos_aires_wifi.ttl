@prefix ex: <http://example.org/wifi#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:wifi_ba_1 a ex:PuntoDeAccesoWiFi ;
    ex:tieneDispositivo ex:device_ba_1 ;
    ex:tieneProveedor ex:provider_ba_1 ;
    ex:tieneSeguridad ex:security_ba_1 ;
    ex:tieneUbicacion ex:location_ba_1 .

ex:device_ba_1 a ex:Dispositivo ;
    ex:nombreDispositivo "Tablet"^^xsd:string .

ex:location_ba_1 a ex:Ubicacion ;
    ex:ciudad "Buenos Aires"^^xsd:string ;
    ex:latitud "-34.6037"^^xsd:float ;
    ex:longitud "-58.3816"^^xsd:float .

ex:provider_ba_1 a ex:Proveedor ;
    ex:nombreProveedor "Telecom Argentina"^^xsd:string .

ex:security_ba_1 a ex:Seguridad ;
    ex:nivelSeguridad "Media"^^xsd:string .

